{
    "name": "rlacerda83/address-search-microservice",
    "description": "Microservice for postal code search.",
    "keywords": ["postalcode", "laravel", "lumen", "microservice", "zip", "cep", "ups", "correios", "mongodb"],
    "license": "MIT",
    "type": "project",
    "require": {
        "laravel/lumen-framework": "5.1.*",
        "vlucas/phpdotenv": "~1.0",
        "jeremeamia/superclosure": "~2.0",
        "dingo/api": "@dev",
        "rlacerda83/query-parser": "@dev",
        "rlacerda83/lumem-elocache": "1.*",
        "guzzlehttp/guzzle": "~6.0",
        "yangqi/htmldom": "dev-master",
        "jenssegers/mongodb": "^2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "fzaninotto/faker": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
